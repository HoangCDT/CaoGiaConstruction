@model Dictionary<int, string>
@{
    if (ViewBag.TotalPage > 1)
    {
        <nav>
            <section class="@(ViewBag.TotalPage <=1 ?"d-none":"")">
                <ul class="pagination pagination-rounded mb-0 justify-content-end">
                    <li class="page-item">
                        <a class="page-link" href="@ViewBag.PageFirstUrl" tabindex="-1">
                            «
                        </a>
                    </li>
                    <li class="page-item">
                        <a class="page-link" href="@ViewBag.PageBackUrl" tabindex="-1">
                            ‹
                        </a>
                    </li>
                    @if (Model.First().Key > 1)
                    {
                        <li class="page-item disabled">
                            <a class="page-link" href="#">...</a>
                        </li>
                    }

                    @foreach (var item in Model)
                    {

                        <li class='@(ViewBag.CurrentPage== item.Key ? "page-item active" : "page-item")'>
                            <a class="page-link" href="@item.Value">@item.Key</a>
                        </li>
                    }

                    @if (Model.Last().Key < ViewBag.TotalPage)
                    {
                        <li class="page-item disabled">
                            <a class="page-link" href="#">...</a>
                        </li>
                    }



                    <li class="page-item">
                        <a class="page-link" href="@ViewBag.PageNextUrl">
                            ›
                        </a>
                    </li>

                    <li class="page-item">
                        <a class="page-link" href="@ViewBag.PageLastUrl">
                            »
                        </a>
                    </li>
                </ul>
            </section>

        </nav>
    }
}
