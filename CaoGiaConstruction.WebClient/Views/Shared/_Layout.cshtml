@inject IProductCategoryService productCategoryService
@inject IAboutService aboutService
@inject ISettingService _settingService;
@{
  var about = await aboutService.GetAboutCacheAsync() ?? new AboutVM();
  var setting = await _settingService.GetSettingCacheAsync() ?? new SettingVM();

  var systemDto = new SystemDto
      {
        About = about,
        Setting = setting
      };
}

